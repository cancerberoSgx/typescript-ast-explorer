{
  "name": "typescript-ast-explorer",
  "version": "0.0.4",
  "description": "Command line tool to explore TypeScript projects and SourceFile abstract syntax tree interactively",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": ".bin/typescript-ast-explorer.js",
  "scripts": {
    "start": "ts-node src/cli/cliMain.ts",
    "watch": "tsc --watch",
    "build": "tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "test-debug": "node --inspect-brk node_modules/ts-node/dist/bin node_modules/jasmine/bin/jasmine.js",
    "prepare": "tsc",
    "docs": "npx markdown-toc README.md -i",
    "lint": " npx ts-refactor format \"src/**/*.ts*\" \"spec/**/*.ts*\" ./formatCodeSettings.json --tsConfigPath ./tsconfig.json --dontAsk",
    "clean": "rm -rf dist",
    "all-publish": "npm run all && npm version patch && npm publish",
    "all": "npm run clean && npm run lint && npm run build && npm run docs && npm test "
  },
  "author": "Sebastian Gurin",
  "license": "MIT",
  "dependencies": {
    "accursed": "0.0.7",
    "ansi-escape-sequences": "^4.1.0",
    "misc-utils-of-mine-generic": "0.2.35",
    "node-keys": "^0.1.5",
    "shelljs": "^0.8.3",
    "ts-morph": "^4.2.0",
    "ts-simple-ast-extra": "^0.3.15",
    "yargs-parser": "^14.0.0"
  },
  "devDependencies": {
    "@types/ansi-escape-sequences": "^4.0.0",
    "@types/jasmine": "^3.4.2",
    "@types/node": "^12.7.11",
    "@types/shelljs": "^0.8.5",
    "cli-driver": "0.2.4",
    "markdown-toc": "^1.2.0",
    "jasmine": "^3.5.0",
    "javascript-stringify": "^2.0.0",
    "ts-node": "^8.4.1",
    "ts-refactor": "0.0.11",
    "typescript": "^3.6.3",
    "watch": "^1.0.2"
  },
  "keywords": [
    "TypeScript",
    "refactor",
    "command line",
    "interactive"
  ],
  "repository": "https://github.com/cancerberoSgx/typescript-ast-explorer"
}