{
  "name": "typescript-ast-explorer",
  "version": "0.0.1",
  "description": "Command line tool to explore TypeScript projects and SourceFile abstract syntax tree interactively",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": "bin/typescript-ast-explorer",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "test": "ts-node node_modules/jasmine/bin/jasmine",
    "test-debug": "node --inspect-brk node_modules/ts-node/dist/bin node_modules/jasmine/bin/jasmine.js",
    "prepare": "tsc",
    "prettier": "npx prettier  --write --arrow-parens avoid --parser typescript --use-tabs false --no-semi true --single-quote true --tab-width 2 --trailing-comma none --end-of-line lf --prose-wrap preserve --print-width 120  --jsx-bracket-same-line true \"src/**/*.ts*\" \"spec/**/*Spec.ts*\"",
    "clean": "rm -rf dist",
    "all": "npm run clean && npm run prettier && npm run build && npm test"
  },
  "author": "Sebastian Gurin",
  "license": "MIT",
  "dependencies": {
    "@phenomnomnominal/tsquery": "^3.0.0",
    "ansi-escape-sequences": "^4.1.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.12",
    "diff": "^4.0.1",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "inquirer-checkbox-plus-prompt": "^1.0.1",
    "misc-utils-of-mine-generic": "0.0.14",
    "shelljs": "^0.8.2",
    "ts-morph": "^1.3.3",
    "ts-simple-ast-extra": "^0.1.4",
    "typescript-ast-util": "^1.1.9",
    "wrap-ansi": "^5.1.0",
    "yargs-parser": "^10.1.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.11",
    "@types/diff": "^4.0.2",
    "@types/inquirer": "^6.0.0",
    "@types/jasmine": "^2.8.16",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.5.2",
    "@types/shelljs": "^0.8.4",
    "cli-driver": "0.2.0",
    "jasmine": "^3.3.1",
    "prettier": "^1.16.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.4.2"
  },
  "keywords": [
    "TypeScript",
    "refactor",
    "command line",
    "interactive"
  ],
  "repository": "https://github.com/cancerberoSgx/typescript-ast-explorer"
}
